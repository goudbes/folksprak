{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
		"plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
	"settings": {
    "react": {
      "version": "detect"
    }
  },
    "plugins": [
        "react"
    ],
    "rules": {
		"linebreak-style": 0
    }
}
